{
  "name": "workflow-builder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "build:frontend": "pnpm --filter frontend build",
    "format": "prettier --write --log-level silent \"**/*.+(css|ts|tsx|json)\"",
    "test": "pnpm --filter frontend test",
    "check": "pnpm lint && pnpm typecheck && pnpm format && knip",
    "pre-commit": "lint-staged",
    "pre-push": "pnpm format && knip"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.12.0",
    "@types/react-dom": "^19.1.0",
    "@types/react-gtm-module": "^2.0.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "knip": "^5.56.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.22.0"
  },
  "engines": {
    "node": "22.12.0",
    "pnpm": "10.9.0"
  },
  "packageManager": "pnpm@10.9.0"
}
